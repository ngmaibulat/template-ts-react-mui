{
  "name": "template-ts-react-mui",
  "version": "0.0.1",
  "description": "Typescript/React application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format:check": "npx prettier src --check",
    "format:fix": "npx prettier src --write",
    "test": "node --test",
    "build:watch": "npx esbuild src/app.tsx --bundle --outdir=public --format=esm --platform=browser --target=es2022 --watch",
    "build": "npx esbuild src/app.tsx --bundle --outdir=public --format=esm --platform=browser --target=es2022",
    "dev:esbuild": "npx esbuild src/app.tsx --bundle --outdir=public --format=esm --platform=browser --target=es2022  --serve=localhost:3000 --servedir=public",
    "serve:esbuild": "npx esbuild --serve=localhost:3000 --servedir=public",
    "dev": "node serve.js",
    "serve": "node serve.js"
  },
  "keywords": [
    "TS",
    "React",
    "MUI"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mui/styled-engine-sc": "^5.11.0",
    "@mui/system": "^5.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/styled-components": "^5.1.26",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.12",
    "prettier": "^2.7.1",
    "servor": "^4.0.2"
  }
}
